{
  "compilerOptions": {
    "target": "es2016",                                  
    "jsx": "react", 
    "module": "commonjs", 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true,
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types"],
    "types": ["node"],
    "allowSyntheticDefaultImports": true
  }
}
